<?xml version="1.0" encoding="UTF-8"?>
<!-- Scope of IPFIX elements -->
<ipfix-elements>
    <scope>
        <pen>0</pen>
        <name>iana</name>
        <biflow mode="split">0</biflow>
    </scope>

    <element>
        <id>0</id>
        <name>b</name>
        <dataType>unsigned64</dataType>
        <dataSemantics>deltaCounter</dataSemantics>
        <units>octets</units>
        <status>current</status>
    </element>

    <element>
        <id>2</id>
        <name>c</name>
        <dataType>unsigned64</dataType>
        <dataSemantics>deltaCounter</dataSemantics>
        <units>packets</units>
        <status>current</status>
    </element>

    <element>
        <id>4</id>
        <name>d</name>
        <dataType>unsigned64</dataType>
        <dataSemantics>deltaCounter</dataSemantics>
        <units>flows</units>
        <status>current</status>
    </element>

    <element>
        <id>6</id>
        <name>e</name>
        <dataType>unsigned8</dataType>
        <dataSemantics>identifier</dataSemantics>
    </element>

    <element>
        <id>8</id>
        <name>f</name>
        <dataType>unsigned8</dataType>
        <dataSemantics>identifier</dataSemantics>
    </element>

    <element>
        <id>10</id>
        <name>g</name>
        <dataType>unsigned16</dataType>
        <dataSemantics>flags</dataSemantics>
    </element>

    <element>
        <id>12</id>
        <name>h</name>
        <dataType>unsigned16</dataType>
        <dataSemantics>identifier</dataSemantics>
    </element>

    <element>
        <id>14</id>
        <name>i</name>
        <dataType>ipv4Address</dataType>
        <dataSemantics>default</dataSemantics>
    </element>

    <!-- TODO: all remaining elements... -->
</ipfix-elements>

 
